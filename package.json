{
  "name": "@tszip/resolve-imports",
  "type": "module",
  "author": "Christian Lewis <ctj.lewis@icloud.com>",
  "version": "2.4.0",
  "license": "MIT",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "boot": "tsc && yarn rollup -c",
    "dev": "tszip dev",
    "build": "tszip build",
    "test": "tszip test",
    "lint": "tszip lint"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tszip lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@tszip/cjs": "^1.4.0",
    "@tszip/tszip": "^1.12.0",
    "@types/fs-extra": "^9.0.12",
    "@types/resolve": "^1.20.1",
    "fs-extra": "^10.0.0",
    "resolve": "^1.20.0"
  }
}
